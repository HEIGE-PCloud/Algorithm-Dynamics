    }
    class Assignment{
        +Guid Uid
        +string Name
        +DateTime DueDate
        +string Description
        +User Assigner
        +AssignmentStatus Status
        +AssignmentType Type
        +Assignment()
    }
    class Submission{
        +int Id
        +Problem Problem
        +string Code
        +DateTime SubmittedTime
        +Submission()
    }
    class SubmissionResult{
        +int Id
        +Submission Submission
        +string StandardOutput
