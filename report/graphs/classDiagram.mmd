        +int id
        +string name
        +Tag()
    }
    class TestCase{
        +int Id
        +string Input
        +string Output
        +TestCase()
    }
    class Problem{
        +int Id
        +string Name
        +Status Status
        +Difficulty Difficulty
        +string Description
        +int TimeLimit
        +int MemoryLimit
        +List<TestCase> TestCases
        +List<Tag> Tags
        +Problem()
    }
